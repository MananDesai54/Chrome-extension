{
    "manifest_version": 2,
    "name": "Magical testcases...",
    "version": "1.0",
    "content_scripts": [
        {
            "matches": [
                "https://codeforces.com/*",
                "https://www.codechef.com/*",
                "https://atcoder.jp/*"
            ],
            "js": ["content.js"]
